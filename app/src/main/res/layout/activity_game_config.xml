                        android:padding="@dimen/margin_width_10dp"/>
                    <TextView
                        android:id="@+id/config_difficulty_fifth"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/config_difficulty_fifth"
                        android:textColor="@color/black"
                        android:textSize="@dimen/text_size_20sp"
                        android:background="@drawable/know_class_common_shape_selector"
                        android:layout_marginEnd="@dimen/margin_width_10dp"
                        android:padding="@dimen/margin_width_10dp"/>
                </GridLayout>
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="@dimen/margin_width_10dp">
                <TextView
                    android:id="@+id/config_contain"
                    android:layout_width="@dimen/margin_width_100dp"
                    android:layout_height="wrap_content"
                    android:text="@string/config_contain"
                    android:textSize="@dimen/text_size_22sp"
                    android:textColor="@color/black"
                    android:layout_marginEnd="@dimen/margin_width_10dp"
                    android:layout_marginTop="@dimen/margin_width_5dp"/>
                <GridLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_toEndOf="@+id/config_contain"
                    android:columnCount="3">
                    <TextView
                        android:id="@+id/config_contain_first"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/config_contain_first"
                        android:textColor="@color/black"
                        android:textSize="@dimen/text_size_20sp"
                        android:background="@drawable/know_class_common_shape_selector"
                        android:layout_marginEnd="@dimen/margin_width_10dp"
                        android:layout_marginBottom="@dimen/margin_width_10dp"
                        android:padding="@dimen/margin_width_10dp"/>
                    <TextView
                        android:id="@+id/config_contain_second"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/config_contain_second"
                        android:textColor="@color/black"
                        android:textSize="@dimen/text_size_20sp"
                        android:background="@drawable/know_class_common_shape_selector"
                        android:layout_marginEnd="@dimen/margin_width_10dp"
                        android:padding="@dimen/margin_width_10dp"/>
                </GridLayout>
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="@dimen/margin_width_10dp">
                <TextView
                    android:id="@+id/config_repeat"
                    android:layout_width="@dimen/margin_width_100dp"
                    android:layout_height="wrap_content"
                    android:text="@string/config_repeat"
                    android:textSize="@dimen/text_size_22sp"
                    android:textColor="@color/black"
                    android:layout_marginEnd="@dimen/margin_width_10dp"
                    android:layout_marginTop="@dimen/margin_width_5dp"/>
                <GridLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_toEndOf="@+id/config_repeat"
                    android:columnCount="3">
                    <TextView
                        android:id="@+id/config_repeat_first"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/config_repeat_first"
                        android:textColor="@color/black"
                        android:textSize="@dimen/text_size_20sp"
                        android:background="@drawable/know_class_common_shape_selector"
                        android:layout_marginEnd="@dimen/margin_width_10dp"
                        android:layout_marginBottom="@dimen/margin_width_10dp"
                        android:padding="@dimen/margin_width_10dp"/>
                    <TextView
                        android:id="@+id/config_repeat_second"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/config_repeat_second"
                        android:textColor="@color/black"
                        android:textSize="@dimen/text_size_20sp"
                        android:background="@drawable/know_class_common_shape_selector"
                        android:layout_marginEnd="@dimen/margin_width_10dp"
                        android:padding="@dimen/margin_width_10dp"/>
                </GridLayout>
            </RelativeLayout>

            <TextView
                android:id="@+id/config_save"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/config_save"
                android:textColor="@color/white"
                android:textSize="@dimen/text_size_24sp"
                android:background="@drawable/config_save_shape"/>
        </LinearLayout>
    </ScrollView>

</LinearLayout>