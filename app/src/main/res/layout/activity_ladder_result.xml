<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <include layout="@layout/include_toolbar_data"/>
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:overScrollMode="never"
        android:scrollbars="none"
        android:fillViewport="true">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:gravity="center">
            <TextView
                android:id="@+id/result_author"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/result_author_text"
                android:textSize="@dimen/text_size_24sp"
                android:textColor="@color/black"
                android:layout_marginTop="@dimen/margin_width_50dp"
                android:layout_marginBottom="@dimen/margin_width_20dp"/>
            <TextView
                android:id="@+id/result_level_head"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/result_level_text"
                android:textColor="@color/black"
                android:textSize="@dimen/text_size_22sp"/>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="@dimen/margin_width_10dp"
                android:text="@string/result_ladder_data_text"
                android:textColor="@color/black"
                android:textSize="@dimen/text_size_20sp"/>
            <TableLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="@dimen/margin_width_10dp">
                <TableRow
                    android:layout_marginBottom="@dimen/margin_width_10dp">
                    <LinearLayout
                        android:layout_width="@dimen/margin_width_0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="horizontal">
                        <TextView
                            style="@style/result_item_style"
                            android:text="@string/result_title_head_text"/>
                        <TextView
                            android:id="@+id/result_title"
                            style="@style/result_item_style"
                            android:text="@string/result_title_text"/>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="@dimen/margin_width_0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="horizontal">
                        <TextView
                            style="@style/result_item_style"
                            android:text="@string/result_level_head_text"/>
                        <TextView
                            android:id="@+id/result_level"
                            style="@style/result_item_style"
                            android:text="@string/result_level_text"/>
                    </LinearLayout>
                </TableRow>
                <TableRow
                    android:layout_marginBottom="@dimen/margin_width_10dp" >
                    <LinearLayout
                        android:layout_width="@dimen/margin_width_0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="horizontal">
                        <TextView
                            style="@style/result_item_style"
                            android:text="@string/result_upgrade_head_text"/>
                        <TextView
                            android:id="@+id/result_upgrade"
                            style="@style/result_item_style"
                            android:text="@string/result_upgrade_text"/>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="@dimen/margin_width_0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="horizontal">
                        <TextView
                            style="@style/result_item_style"
                            android:text="@string/result_upgrade_total_head_text"/>
                        <TextView
                            android:id="@+id/result_upgrade_total"
                            style="@style/result_item_style"
                            android:text="@string/result_upgrade_total_text"/>
                    </LinearLayout>
                </TableRow>
                <TableRow
                    android:layout_marginBottom="@dimen/margin_width_10dp" >
                    <LinearLayout
                        android:layout_width="@dimen/margin_width_0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="horizontal">
                        <TextView
                            style="@style/result_item_style"
                            android:text="@string/result_difficulty_head_text"/>
                        <TextView
                            android:id="@+id/result_difficulty"
                            style="@style/result_item_style"
                            android:text="@string/result_difficulty_text"/>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="@dimen/margin_width_0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="horizontal">
                        <TextView
                            style="@style/result_item_style"
                            android:text="@string/result_highest_head_text"/>
                        <TextView
                            android:id="@+id/result_highest"
                            style="@style/result_item_style"
                            android:text="@string/result_highest_text"/>
                    </LinearLayout>
                </TableRow>
                <TableRow
                    android:layout_marginBottom="@dimen/margin_width_10dp"  >
                    <LinearLayout
                        android:layout_width="@dimen/margin_width_0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="horizontal">
                        <TextView
                            style="@style/result_item_style"
                            android:text="@string/result_fail_head_text"/>
                        <TextView
                            android:id="@+id/result_fail"
                            style="@style/result_item_style"
                            android:text="@string/result_fail_text"/>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="@dimen/margin_width_0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="horizontal">
                        <TextView
                            style="@style/result_item_style"
                            android:text="@string/result_time_head_text"/>
                        <TextView
                            android:id="@+id/result_time"
                            style="@style/result_item_style"
                            android:text="@string/result_time_text"/>
                    </LinearLayout>
                </TableRow>
                <TableRow
                    android:layout_marginBottom="@dimen/margin_width_10dp"   >
                    <LinearLayout
                        android:layout_width="@dimen/margin_width_0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="horizontal">
                        <TextView
                            style="@style/result_item_style"
                            android:text="@string/result_total_time_head_text"/>
                        <TextView
                            android:id="@+id/result_total_time"
                            style="@style/result_item_style"
                            android:text="@string/result_total_time_text"/>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="@dimen/margin_width_0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="horizontal">
                        <TextView
                            style="@style/result_item_style"
                            android:text="@string/result_total_head_text"/>
                        <TextView
                            android:id="@+id/result_total"
                            style="@style/result_item_style"
                            android:text="@string/result_total_text"/>
                    </LinearLayout>
                </TableRow>
                <TableRow
                    android:layout_marginBottom="@dimen/margin_width_10dp"  >
                    <LinearLayout
                        android:layout_width="@dimen/margin_width_0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="horizontal">
                        <TextView
                            style="@style/result_item_style"
                            android:text="@string/result_master_head_text"/>
                        <TextView
                            android:id="@+id/result_master"
                            style="@style/result_item_style"
                            android:text="@string/result_master_text"/>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="@dimen/margin_width_0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="horizontal">
                        <TextView
                            style="@style/result_item_style"
                            android:text="@string/result_chance_head_text"/>
                        <TextView
                            android:id="@+id/result_chance"
                            style="@style/result_item_style"
                            android:text="@string/result_chance_text"/>
                    </LinearLayout>
                </TableRow>
            </TableLayout>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/result_ladder_use_text"
                android:textSize="@dimen/text_size_20sp"
                android:textColor="@color/black"
                android:padding="@dimen/margin_width_10dp"/>
            <TableLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="@dimen/margin_width_10dp">
                <TableRow
                    android:layout_marginBottom="@dimen/margin_width_10dp"  >
                    <LinearLayout
                        android:layout_width="@dimen/margin_width_0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="horizontal">
                        <TextView
                            style="@style/result_item_style"
                            android:text="@string/result_start_time_head_text"/>
                        <TextView
                            android:id="@+id/result_start_time"
                            style="@style/result_item_style"
                            android:text="@string/result_start_time_text"/>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="@dimen/margin_width_0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="horizontal">
                        <TextView
                            style="@style/result_item_style"
                            android:text="@string/result_day_head_text"/>
                        <TextView
                            android:id="@+id/result_day"
                            style="@style/result_item_style"
                            android:text="@string/result_day_text"/>
                    </LinearLayout>
                </TableRow>
                <TableRow
                    android:layout_marginBottom="@dimen/margin_width_10dp"       >
                    <LinearLayout
                        android:layout_width="@dimen/margin_width_0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="horizontal">
                        <TextView
                            style="@style/result_item_style"
                            android:text="@string/result_dare_total_head_text"/>
                        <TextView
                            android:id="@+id/result_dare_total"
                            style="@style/result_item_style"
                            android:text="@string/result_dare_total_text"/>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="@dimen/margin_width_0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="horizontal">
                        <TextView
                            style="@style/result_item_style"
                            android:text="@string/result_fail_total_head_text"/>
                        <TextView
                            android:id="@+id/result_fail_total"
                            style="@style/result_item_style"
                            android:text="@string/result_fail_total_text"/>
                    </LinearLayout>
                </TableRow>
                <TableRow
                    android:layout_marginBottom="@dimen/margin_width_10dp"    >
                    <LinearLayout
                        android:layout_width="@dimen/margin_width_0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="horizontal">
                        <TextView
                            style="@style/result_item_style"
                            android:text="@string/result_win_total_head_text"/>
                        <TextView
                            android:id="@+id/result_win_total"
                            style="@style/result_item_style"
                            android:text="@string/result_win_total_text"/>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="@dimen/margin_width_0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="horizontal">
                        <TextView
                            style="@style/result_item_style"
                            android:text="@string/result_long_time_head_text"/>
                        <TextView
                            android:id="@+id/result_long_time"
                            style="@style/result_item_style"
                            android:text="@string/result_long_time_text"/>
                    </LinearLayout>
                </TableRow>
                <TableRow
                    android:layout_marginBottom="@dimen/margin_width_10dp">
                    <LinearLayout
                        android:layout_width="@dimen/margin_width_0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="horizontal">
                        <TextView
                            style="@style/result_item_style"
                            android:text="@string/result_highest_count_head_text"/>
                        <TextView
                            android:id="@+id/result_highest_count"
                            style="@style/result_item_style"
                            android:text="@string/result_highest_count_text"/>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="@dimen/margin_width_0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="horizontal">
                        <TextView
                            style="@style/result_item_style"
                            android:text="@string/result_score_head_text"/>
                        <TextView
                            android:id="@+id/result_score"
                            style="@style/result_item_style"
                            android:text="@string/result_score_text"/>
                    </LinearLayout>
                </TableRow>
                <TableRow
                    android:layout_marginBottom="@dimen/margin_width_10dp">
                    <LinearLayout
                        android:layout_width="@dimen/margin_width_0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="horizontal">
                        <TextView
                            style="@style/result_item_style"
                            android:text="@string/result_short_time_head_text"/>
                        <TextView
                            android:id="@+id/result_short_time"
                            style="@style/result_item_style"
                            android:text="@string/result_short_time_text"/>
                    </LinearLayout>
                </TableRow>
            </TableLayout>
        </LinearLayout>
    </ScrollView>
</LinearLayout>