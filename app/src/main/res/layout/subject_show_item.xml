                android:layout_height="wrap_content"
                android:text="@string/subject_number_second"
                android:textSize="@dimen/text_size_24sp"
                android:textColor="@color/black"
                android:layout_marginEnd="@dimen/margin_width_10dp"/>
            <ImageView
                android:id="@+id/radio_subject_second_img"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/subject_wrong_normal"
                android:layout_marginEnd="@dimen/margin_width_10dp"/>
            <TextView
                android:id="@+id/radio_subject_second_title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/radio_subject_second_title"
                android:textColor="@color/black"
                android:textSize="@dimen/text_size_24sp"/>
        </LinearLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/margin_width_1dp"
            android:background="@color/white_1"
            android:layout_marginTop="@dimen/margin_width_5dp"/>

        <LinearLayout
            android:id="@+id/radio_subject_third"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:gravity="center|start">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/subject_number_third"
                android:textSize="@dimen/text_size_24sp"
                android:textColor="@color/black"
                android:layout_marginEnd="@dimen/margin_width_10dp"/>
            <ImageView
                android:id="@+id/radio_subject_third_img"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/subject_wrong_normal"
                android:layout_marginEnd="@dimen/margin_width_10dp"/>
            <TextView
                android:id="@+id/radio_subject_third_title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/radio_subject_third_title"
                android:textColor="@color/black"
                android:textSize="@dimen/text_size_24sp"/>
        </LinearLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/margin_width_1dp"
            android:background="@color/white_1"
            android:layout_marginTop="@dimen/margin_width_5dp" />
        <LinearLayout
            android:id="@+id/radio_subject_fourth"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
