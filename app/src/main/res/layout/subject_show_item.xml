<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:paddingStart="@dimen/margin_width_10dp"
    android:paddingEnd="@dimen/margin_width_10dp"
    android:paddingTop="@dimen/margin_width_10dp"
    android:gravity="center">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_marginBottom="@dimen/margin_width_10dp">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:gravity="center|start">
            <ImageView
                android:id="@+id/subject_indicate"
                android:layout_width="@dimen/margin_width_20dp"
                android:layout_height="@dimen/margin_width_20dp"
                android:src="@drawable/subject_indicate_normal"
                android:layout_marginEnd="@dimen/margin_width_10dp"/>
            <TextView
                android:id="@+id/subject_problem_title"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/subject_problem_title"
                android:textColor="@color/black"
                android:textSize="@dimen/text_size_24sp"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:gravity="center|end">
            <TextView
                android:id="@+id/subject_details"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/subject_details_show"
                android:textSize="@dimen/text_size_20sp"
                android:textColor="@color/black"
                android:paddingEnd="@dimen/margin_width_10dp"/>
            <TextView
                android:id="@+id/subject_class"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/subject_class_first"
                android:textColor="@color/black"
                android:textSize="@dimen/text_size_18sp"
                android:paddingEnd="@dimen/margin_width_10dp"/>
            <TextView
                android:id="@+id/subject_time"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/subject_time"
                android:textSize="@dimen/text_size_16sp"
                android:textColor="@color/black_2"/>
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
