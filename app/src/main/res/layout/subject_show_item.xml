                android:layout_height="wrap_content"
                android:text="@string/subject_number_fourth"
                android:textSize="@dimen/text_size_24sp"
                android:textColor="@color/black"
                android:layout_marginEnd="@dimen/margin_width_10dp"/>
            <ImageView
                android:id="@+id/radio_subject_fourth_img"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/subject_wrong_normal"
                android:layout_marginEnd="@dimen/margin_width_10dp"/>
            <TextView
                android:id="@+id/radio_subject_fourth_title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/radio_subject_fourth_title"
                android:textColor="@color/black"
                android:textSize="@dimen/text_size_24sp"/>
        </LinearLayout>
    </LinearLayout>

